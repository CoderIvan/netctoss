<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
    "http://struts.apache.org/dtds/struts-2.1.7.dtd">
<struts>
	<package name="role-package" extends="main-default" namespace="/role">
		<action name="index" class="com.tarena.netctoss.action.role.RoleInfoListAction" method="index">
			<result>/WEB-INF/JSP/role/role_list.jsp</result>
		</action>
		<action name="toAdd" class="com.tarena.netctoss.action.role.RoleInfoAddAction" method="toAdd">
			<result>/WEB-INF/JSP/role/role_add.jsp</result>
		</action>
		<action name="add" class="com.tarena.netctoss.action.role.RoleInfoAddAction" method="add"/>
		<action name="toUpdate" class="com.tarena.netctoss.action.role.RoleInfoToUpdateAction" method="toUpdate">
			<result>/WEB-INF/JSP/role/role_modi.jsp</result>
		</action>
		<action name="update" class="com.tarena.netctoss.action.role.RoleInfoUpdateAction" method="update"/>
		<action name="delete" class="com.tarena.netctoss.action.role.RoleInfoDeleteAction" method="delete"/>
	</package>
</struts>